@article{Grytczuk2008,
abstract = {A sequence S = s1 s2 ... sn is said to be nonrepetitive if no two adjacent blocks of S are the same. A celebrated 1906 theorem of Thue asserts that there are arbitrarily long nonrepetitive sequences over the set {\{} 0, 1, 2 {\}}. This result is the starting point of Combinatorics on Words-a wide area with many deep results, sophisticated methods, important applications and intriguing open problems. The main purpose of this survey is to present a range of new directions relating Thue sequences more closely to Graph Theory, Combinatorial Geometry, and Number Theory. For instance, one may consider graph colorings avoiding repetitions on paths, or colorings of points in the plane avoiding repetitions on straight lines. Besides presenting a variety of new challenges we also recall some older problems of this area. {\textcopyright} 2007 Elsevier B.V. All rights reserved.},
author = {Grytczuk, Jaros{\l}aw},
doi = {10.1016/j.disc.2007.08.039},
file = {:home/ted/Dokumente/BA/thue{\_}problems{\_}overview.pdf:pdf},
issn = {0012365X},
journal = {Discrete Mathematics},
keywords = {Avoidable pattern,Graph coloring,Nonrepetitive sequence},
number = {19},
pages = {4419--4429},
title = {{Thue type problems for graphs, points, and numbers}},
volume = {308},
year = {2008}
}
