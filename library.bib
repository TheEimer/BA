@article{Skrabulakova2015,
abstract = {We say that a vertex colouring {\$}\backslashvarphi{\$} of a graph {\$}G{\$} is nonrepetitive if there is no positive integer {\$}n{\$} and a path on {\$}2n{\$} vertices {\$}v{\_}{\{}1{\}}\backslashldots v{\_}{\{}2n{\}}{\$} in {\$}G{\$} such that the associated sequence of colours {\$}\backslashvarphi(v{\_}{\{}1{\}})\backslashldots\backslashvarphi(v{\_}{\{}2n{\}}){\$} satisfy {\$}\backslashvarphi(v{\_}{\{}i{\}})=\backslashvarphi(v{\_}{\{}i+n{\}}){\$} for all {\$}i=1,2,\backslashdots,n{\$}. The minimum number of colours in a nonrepetitive vertex colouring of {\$}G{\$} is the Thue chromatic number {\$}\backslashpi (G){\$}. For the case of vertex list colourings the Thue choice number {\$}\backslashpi{\_}{\{}l{\}}(G){\$} of {\$}G{\$} denotes the smallest integer {\$}k{\$} such that for every list assignment {\$}L:V(G)\backslashrightarrow 2{\^{}}{\{}\backslashmathbb{\{}N{\}}{\}}{\$} with minimum list length at least {\$}k{\$}, there is a nonrepetitive vertex colouring of {\$}G{\$} from the assigned lists. Recently it was proved that the Thue chromatic number and the Thue choice number of the same graph may have an arbitrary large difference in some classes of graphs. Here we give an overview of the known results where we compare these two parameters for several families of graphs and we also give a list of open problems on this topic.},
archivePrefix = {arXiv},
arxivId = {1508.02559},
author = {{\v{S}}krabuľ{\'{a}}kov{\'{a}}, Erika},
eprint = {1508.02559},
file = {:home/ted/Dokumente/BA/thue{\_}vs{\_}choice.pdf:pdf},
keywords = {05c15,itive colouring,mathematics subject classifications,nonrepet-,nonrepetitive sequence,square-free colouring,thue choice number,thue chromatic number},
pages = {1--19},
title = {{The Thue choice number versus the Thue chromatic number of graphs}},
url = {http://arxiv.org/abs/1508.02559},
year = {2015}
}
